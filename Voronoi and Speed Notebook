   "cell_type": "code",
   "execution_count": 22,
   "id": "d6f69fed",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-12-17T14:42:34.319836Z",
     "iopub.status.busy": "2025-12-17T14:42:34.319449Z",
     "iopub.status.idle": "2025-12-17T14:42:34.338210Z",
     "shell.execute_reply": "2025-12-17T14:42:34.337134Z"
    },
    "papermill": {
     "duration": 0.029781,
     "end_time": "2025-12-17T14:42:34.339918",
     "exception": false,
     "start_time": "2025-12-17T14:42:34.310137",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def voronoi_finite_polygons_2d(vor, radius=1000):\n",
    "    new_regions = []\n",
    "    new_vertices = vor.vertices.tolist()\n",
    "\n",
    "    center = vor.points.mean(axis=0)\n",
    "    all_ridges = {}\n",
    "\n",
    "    for (p1, p2), (v1, v2) in zip(vor.ridge_points, vor.ridge_vertices):\n",
    "        all_ridges.setdefault(p1, []).append((p2, v1, v2))\n",
    "        all_ridges.setdefault(p2, []).append((p1, v1, v2))\n",
    "\n",
    "    for p1, region_idx in enumerate(vor.point_region):\n",
    "        vertices = vor.regions[region_idx]\n",
    "        if all(v >= 0 for v in vertices):\n",
    "            new_regions.append(vertices)\n",
    "            continue\n",
    "\n",
    "        ridges = all_ridges[p1]\n",
    "        region = [v for v in vertices if v >= 0]\n",
    "\n",
    "        for p2, v1, v2 in ridges:\n",
    "            if v1 >= 0 and v2 >= 0:\n",
    "                continue\n",
    "\n",
    "            v = v1 if v1 >= 0 else v2\n",
    "            t = vor.points[p2] - vor.points[p1]\n",
    "            t_norm = np.linalg.norm(t)\n",
    "            if t_norm == 0:\n",
    "                continue\n",
    "            t = t / t_norm\n",
    "            n = np.array([-t[1], t[0]])\n",
    "\n",
    "            midpoint = vor.points[[p1, p2]].mean(axis=0)\n",
    "            direction = np.sign(np.dot(midpoint - center, n)) * n\n",
    "            far_point = vor.vertices[v] + direction * radius\n",
    "\n",
    "            new_vertices.append(far_point.tolist())\n",
    "            region.append(len(new_vertices) - 1)\n",
    "\n",
    "        new_regions.append(region)\n",
    "\n",
    "    return new_regions, np.asarray(new_vertices)\n",
    "\n",
    "def _safe_clipped_area(pts, bounds_poly):\n",
    "    c = pts.mean(axis=0)\n",
    "    ang = np.arctan2(pts[:, 1] - c[1], pts[:, 0] - c[0])\n",
    "    pts = pts[np.argsort(ang)]\n",
    "\n",
    "    poly = Polygon(pts)\n",
    "\n",
    "    # Repair invalid polygons\n",
    "    if not poly.is_valid:\n",
    "        poly = poly.buffer(0)\n",
    "\n",
    "    if poly.is_empty or (not poly.is_valid):\n",
    "        return 0.0\n",
    "\n",
    "    try:\n",
    "        clipped = poly.intersection(bounds_poly)\n",
    "    except GEOSException:\n",
    "        clipped = poly.buffer(0).intersection(bounds_poly)\n",
    "\n",
    "    if clipped.is_empty:\n",
    "        return 0.0\n",
    "    return float(clipped.area)\n",
    "\n",
    "\n",
    "# add vor_area to play_df\n",
    "def add_voronoi_area_by_frame(\n",
    "    play_df,\n",
    "    x_col=\"x\",\n",
    "    y_col=\"y\",\n",
    "    player_col=\"nfl_id\",\n",
    "    frame_col=\"frame_id\",\n",
    "    field_xmin=0.0,\n",
    "    field_xmax=120.0,\n",
    "    field_ymin=0.0,\n",
    "    field_ymax=53.3,\n",
    "    jitter_eps=1e-6,\n",
    "):\n",
    "\n",
    "    df = play_df.copy()\n",
    "\n",
    "    required = {frame_col, player_col, x_col, y_col}\n",
    "    missing = sorted(list(required - set(df.columns)))\n",
    "    if missing:\n",
    "        raise ValueError(f\"Missing columns: {missing}\")\n",
    "\n",
    "    bounds_poly = box(field_xmin, field_ymin, field_xmax, field_ymax)\n",
    "\n",
    "    out_frames = []\n",
    "    for fr, fdf in df.groupby(frame_col, sort=True):\n",
    "        fdf = fdf.copy().reset_index(drop=True)\n",
    "\n",
    "        if len(fdf) < 4:\n",
    "            fdf[\"vor_area\"] = np.nan\n",
    "            out_frames.append(fdf)\n",
    "            continue\n",
    "\n",
    "        coords = fdf[[x_col, y_col]].to_numpy()\n",
    "        # detect duplicates\n",
    "        _, counts = np.unique(coords, axis=0, return_counts=True)\n",
    "        if counts.max() > 1:\n",
    "            noise = np.random.normal(scale=jitter_eps, size=coords.shape)\n",
    "            coords = coords + noise\n",
    "\n",
    "        # Build Voronoi\n",
    "        vor = Voronoi(coords)\n",
    "        regions, vertices = voronoi_finite_polygons_2d(vor, radius=1000)\n",
    "\n",
    "        # Compute area per row\n",
    "        areas = []\n",
    "        for i, region in enumerate(regions):\n",
    "            pts = vertices[region]\n",
    "            area = _safe_clipped_area(pts, bounds_poly)\n",
    "            areas.append(area)\n",
    "\n",
    "        fdf[\"vor_area\"] = areas\n",
    "        out_frames.append(fdf)\n",
    "\n",
    "    return pd.concat(out_frames, ignore_index=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "0cef17f9",
