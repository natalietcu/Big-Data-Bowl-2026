   "cell_type": "code",
   "execution_count": 10,
   "id": "469dc9b3",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-12-17T14:42:30.638649Z",
     "iopub.status.busy": "2025-12-17T14:42:30.637903Z",
     "iopub.status.idle": "2025-12-17T14:42:30.658060Z",
     "shell.execute_reply": "2025-12-17T14:42:30.657051Z"
    },
    "papermill": {
     "duration": 0.028223,
     "end_time": "2025-12-17T14:42:30.659718",
     "exception": false,
     "start_time": "2025-12-17T14:42:30.631495",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "FIELD_X_MIN, FIELD_X_MAX = 0.0, 120.0\n",
    "FIELD_Y_MIN, FIELD_Y_MAX = 0.0, 53.3\n",
    "\n",
    "COLOR_DEF = \"rgba(30, 90, 255, 0.28)\"\n",
    "COLOR_OFF = \"rgba(255, 70, 70, 0.28)\"\n",
    "LINE_COLOR = \"rgba(0,0,0,0.15)\"\n",
    "\n",
    "def voronoi_polygons_for_frame(frame_df):\n",
    "    def_pts = frame_df.loc[frame_df[\"player_side\"] == \"Defense\", [\"x\", \"y\"]].to_numpy()\n",
    "    off_pts = frame_df.loc[frame_df[\"player_side\"] == \"Offense\", [\"x\", \"y\"]].to_numpy()\n",
    "    tgt_pts = frame_df.loc[frame_df[\"player_role\"] == \"Targeted Receiver\", [\"x\", \"y\"]].to_numpy()\n",
    "\n",
    "    if (len(def_pts) + len(off_pts)) < 2:\n",
    "        return [], def_pts, off_pts, tgt_pts\n",
    "\n",
    "    # Add ghost points\n",
    "    pad = 40.0\n",
    "    ghost = np.array([\n",
    "        [FIELD_X_MIN - pad, FIELD_Y_MIN - pad],\n",
    "        [FIELD_X_MIN - pad, FIELD_Y_MAX + pad],\n",
    "        [FIELD_X_MAX + pad, FIELD_Y_MIN - pad],\n",
    "        [FIELD_X_MAX + pad, FIELD_Y_MAX + pad],\n",
    "    ])\n",
    "    all_points = np.vstack([def_pts, off_pts, ghost])\n",
    "    vor = Voronoi(all_points)\n",
    "\n",
    "    regions = []\n",
    "    n_real = len(def_pts) + len(off_pts)\n",
    "    for p_idx in range(n_real):\n",
    "        region_index = vor.point_region[p_idx]\n",
    "        region = vor.regions[region_index]\n",
    "        if not region or (-1 in region):\n",
    "            continue\n",
    "        poly = vor.vertices[region]\n",
    "        regions.append({\n",
    "            \"x\": poly[:, 0].tolist(),\n",
    "            \"y\": poly[:, 1].tolist(),\n",
    "            \"team\": \"Defense\" if p_idx < len(def_pts) else \"Offense\"\n",
    "        })\n",
    "    return regions, def_pts, off_pts, tgt_pts\n",
    "\n",
    "\n",
    "def build_voronoi_animation(play_df, ball_x=None, ball_y=None, title=\"Voronoi Ownership + Defensive Hull\"):\n",
    "    frames_out = []\n",
    "    frame_ids = sorted(play_df[\"frame_id\"].unique())\n",
    "\n",
    "    for fid in frame_ids:\n",
    "        fdf = play_df[play_df[\"frame_id\"] == fid]\n",
    "        regions, def_pts, off_pts, tgt_pts = voronoi_polygons_for_frame(fdf)\n",
    "        traces = []\n",
    "\n",
    "        # Voronoi ownership polygons\n",
    "        for reg in regions:\n",
    "            color = COLOR_DEF if reg[\"team\"] == \"Defense\" else COLOR_OFF\n",
    "            traces.append(go.Scatter(\n",
    "                x=reg[\"x\"] + [reg[\"x\"][0]],\n",
    "                y=reg[\"y\"] + [reg[\"y\"][0]],\n",
    "                mode=\"lines\",\n",
    "                line=dict(color=LINE_COLOR, width=1),\n",
    "                fill=\"toself\",\n",
    "                fillcolor=color,\n",
    "                hoverinfo=\"skip\",\n",
    "                showlegend=False\n",
    "            ))\n",
    "\n",
    "        # Defensive convex hull\n",
    "        if len(def_pts) >= 3:\n",
    "            hull = ConvexHull(def_pts)\n",
    "            hull_pts = def_pts[hull.vertices]\n",
    "            traces.append(go.Scatter(\n",
    "                x=np.append(hull_pts[:, 0], hull_pts[0, 0]),\n",
    "                y=np.append(hull_pts[:, 1], hull_pts[0, 1]),\n",
    "                mode=\"lines\",\n",
    "                line=dict(color=\"rgba(0,0,200,0.8)\", width=2),\n",
    "                name=\"Defensive Hull\"\n",
    "            ))\n",
    "\n",
    "        # Players\n",
    "        traces.append(go.Scatter(x=def_pts[:, 0], y=def_pts[:, 1],\n",
    "                                 mode=\"markers\", marker=dict(color=\"blue\", size=6), name=\"Defenders\"))\n",
    "        traces.append(go.Scatter(x=off_pts[:, 0], y=off_pts[:, 1],\n",
    "                                 mode=\"markers\", marker=dict(color=\"red\", size=6), name=\"Offense\"))\n",
    "\n",
    "        # Targeted receiver\n",
    "        if len(tgt_pts) > 0:\n",
    "            traces.append(go.Scatter(x=tgt_pts[:, 0], y=tgt_pts[:, 1],\n",
    "                                     mode=\"markers\", marker=dict(color=\"gold\", size=14, symbol=\"star\"),\n",
    "                                     name=\"Targeted Receiver\"))\n",
    "\n",
    "        # Fixed ball landing point\n",
    "        if ball_x is not None and ball_y is not None:\n",
    "            traces.append(go.Scatter(x=[ball_x], y=[ball_y],\n",
    "                                     mode=\"markers\", marker=dict(color=\"brown\", size=10, symbol=\"circle\"),\n",
    "                                     name=\"Ball Landing\"))\n",
    "\n",
    "        frames_out.append(go.Frame(name=str(fid), data=traces))\n",
    "\n",
    "    base_data = frames_out[0].data if frames_out else []\n",
    "\n",
    "    fig = go.Figure(\n",
    "        data=base_data,\n",
    "        frames=frames_out,\n",
    "        layout=go.Layout(\n",
    "            title=title,\n",
    "            xaxis=dict(range=[FIELD_X_MIN, FIELD_X_MAX], title=\"X (yards)\"),\n",
    "            yaxis=dict(range=[FIELD_Y_MIN, FIELD_Y_MAX], title=\"Y (yards)\"),\n",
    "            width=950, height=480, showlegend=True,\n",
    "\n",
    "            sliders=[{\n",
    "                \"x\": 0.10, \"y\": -0.10, \"len\": 0.8,\n",
    "                \"currentvalue\": {\"prefix\": \"Frame: \"},\n",
    "                \"steps\": [\n",
    "                    {\"label\": str(fid), \"method\": \"animate\",\n",
    "                     \"args\": [[str(fid)], {\"mode\": \"immediate\",\n",
    "                                           \"frame\": {\"duration\": 0, \"redraw\": True}}]}\n",
    "                    for fid in frame_ids\n",
    "                ]\n",
    "            }]\n",
    "        )\n",
    "    )\n",
    "    return fig\n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "73ad96a9",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-12-17T14:42:30.670643Z",
     "iopub.status.busy": "2025-12-17T14:42:30.670329Z",
     "iopub.status.idle": "2025-12-17T14:42:30.677156Z",
     "shell.execute_reply": "2025-12-17T14:42:30.675743Z"
    },
    "papermill": {
     "duration": 0.014356,
     "end_time": "2025-12-17T14:42:30.678763",
     "exception": false,
     "start_time": "2025-12-17T14:42:30.664407",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def project_positions_with_motion(frame_df, lookahead_sec=0.5):\n",
    "\n",
    "    df = frame_df.copy()\n",
    "\n",
    "    # Direction (dir) in degrees -> radians\n",
    "    theta = np.deg2rad(df[\"dir\"].fillna(0.0).to_numpy())\n",
    "    v = df[\"s\"].fillna(0.0).to_numpy()      # speed (yd/s)\n",
    "    a = df[\"a\"].fillna(0.0).to_numpy()      # acceleration (yd/s^2)\n",
    "    dt = float(lookahead_sec)\n",
    "\n",
    "    # Kinematic projection formula along direction of motion:\n",
    "    dx = v * np.cos(theta) * dt + 0.5 * a * np.cos(theta) * dt * dt\n",
    "    dy = v * np.sin(theta) * dt + 0.5 * a * np.sin(theta) * dt * dt\n",
    "\n",
    "    df[\"x_eff\"] = df[\"x\"].to_numpy() + dx\n",
    "    df[\"y_eff\"] = df[\"y\"].to_numpy() + dy\n",
    "\n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "0c9a2b60",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-12-17T14:42:30.689960Z",
     "iopub.status.busy": "2025-12-17T14:42:30.689599Z",
     "iopub.status.idle": "2025-12-17T14:42:30.817820Z",
     "shell.execute_reply": "2025-12-17T14:42:30.816681Z"
    },
    "papermill": {
     "duration": 0.136051,
     "end_time": "2025-12-17T14:42:30.819596",
     "exception": false,
     "start_time": "2025-12-17T14:42:30.683545",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def voronoi_polygons_for_frame_motion(frame_df, lookahead_sec=0.5):\n",
    " \n",
    "    # 1) Project players forward using speed & acceleration\n",
    "    df_eff = project_positions_with_motion(frame_df, lookahead_sec=lookahead_sec)\n",
    "\n",
    "    # identify QB rows\n",
    "    qb_mask = np.zeros(len(df_eff), dtype=bool)\n",
    "    if \"position\" in df_eff.columns:\n",
    "        qb_mask |= (df_eff[\"position\"] == \"QB\").to_numpy()\n",
    "    if \"player_role\" in df_eff.columns:\n",
    "        qb_mask |= (df_eff[\"player_role\"] == \"Passer\").to_numpy()\n",
    "\n",
    "    # use nfl_id as the player identifier\n",
    "    if \"nfl_id\" not in df_eff.columns:\n",
    "        raise KeyError(\"Expected column 'nfl_id' in tracking data\")\n",
    "    id_col = \"nfl_id\"\n",
    "\n",
    "    # masks for non-QB defense/offense\n",
    "    def_mask = (df_eff[\"player_side\"] == \"Defense\") & (~qb_mask)\n",
    "    off_mask = (df_eff[\"player_side\"] == \"Offense\") & (~qb_mask)\n",
    "\n",
    "    # Effective positions used for Voronoi ownership (QB excluded)\n",
    "    def_eff = df_eff.loc[def_mask, [\"x_eff\", \"y_eff\"]].to_numpy()\n",
    "    off_eff = df_eff.loc[off_mask, [\"x_eff\", \"y_eff\"]].to_numpy()\n",
    "\n",
    "    # IDs aligned with those EFFECTIVE positions\n",
    "    def_ids = df_eff.loc[def_mask, id_col].to_numpy()\n",
    "    off_ids = df_eff.loc[off_mask, id_col].to_numpy()\n",
    "\n",
    "    # targeted receiver mask, positions, ids\n",
    "    tgt_mask = (df_eff[\"player_role\"] == \"Targeted Receiver\") & (~qb_mask)\n",
    "    tgt_actual = df_eff.loc[tgt_mask, [\"x\", \"y\"]].to_numpy()\n",
    "    tgt_ids = df_eff.loc[tgt_mask, id_col].to_numpy()\n",
    "\n",
    "    # Actual current positions & IDs for plotting dots & hull\n",
    "    def_actual = df_eff.loc[def_mask, [\"x\", \"y\"]].to_numpy()\n",
    "    off_actual = df_eff.loc[off_mask, [\"x\", \"y\"]].to_numpy()\n",
    "    tgt_actual = df_eff.loc[df_eff[\"player_role\"] == \"Targeted Receiver\", [\"x\", \"y\"]].to_numpy()\n",
    "\n",
    "    if (len(def_eff) + len(off_eff)) < 2:\n",
    "        return [], def_actual, off_actual, tgt_actual, def_actual_ids, off_actual_ids\n",
    "\n",
    "    # 2) Add ghost points to bound field & build Voronoi ON EFFECTIVE POSITIONS\n",
    "    pad = 40.0\n",
    "    ghost = np.array([\n",
    "        [FIELD_X_MIN - pad, FIELD_Y_MIN - pad],\n",
    "        [FIELD_X_MIN - pad, FIELD_Y_MAX + pad],\n",
    "        [FIELD_X_MAX + pad, FIELD_Y_MIN - pad],\n",
    "        [FIELD_X_MAX + pad, FIELD_Y_MAX + pad],\n",
    "    ])\n",
    "\n",
    "    all_points = np.vstack([def_eff, off_eff, ghost])\n",
    "    vor = Voronoi(all_points)\n",
    "\n",
    "    regions = []\n",
    "    n_real = len(def_eff) + len(off_eff)\n",
    "\n",
    "    for p_idx in range(n_real):\n",
    "        region_index = vor.point_region[p_idx]\n",
    "        region = vor.regions[region_index]\n",
    "        if not region or (-1 in region):\n",
    "            continue\n",
    "\n",
    "        poly = vor.vertices[region]\n",
    "\n",
    "        if p_idx < len(def_eff):\n",
    "            team = \"Defense\"\n",
    "            player_nfl_id = def_ids[p_idx]\n",
    "        else:\n",
    "            team = \"Offense\"\n",
    "            off_idx = p_idx - len(def_eff)\n",
    "            player_nfl_id = off_ids[off_idx]\n",
    "\n",
    "        regions.append({\n",
    "            \"x\": poly[:, 0].tolist(),\n",
    "            \"y\": poly[:, 1].tolist(),\n",
    "            \"team\": team,\n",
    "            \"nfl_id\": player_nfl_id,      # space-owner id\n",
    "        })\n",
    "\n",
    "    return regions, def_actual, off_actual, tgt_actual, def_ids, off_ids, tgt_ids"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "26767bc0",
   "metadata": {
