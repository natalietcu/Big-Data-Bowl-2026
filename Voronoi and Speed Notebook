   "source": [
    "def build_voronoi_animation_motion(play_df,\n",
    "                                   ball_x=None,\n",
    "                                   ball_y=None,\n",
    "                                   title=\"Motion-Aware Voronoi Ownership\",\n",
    "                                   lookahead_sec=0.5):\n",
    "\n",
    "\n",
    "    frames_out = []\n",
    "    frame_ids = sorted(play_df[\"frame_id\"].unique())\n",
    "\n",
    "    for idx, fid in enumerate(frame_ids):\n",
    "        fdf = play_df[play_df[\"frame_id\"] == fid]\n",
    "\n",
    "        # QB positions from original frame data\n",
    "        qb_mask = np.zeros(len(fdf), dtype=bool)\n",
    "        if \"position\" in fdf.columns:\n",
    "            qb_mask |= (fdf[\"position\"] == \"QB\").to_numpy()\n",
    "        if \"player_role\" in fdf.columns:\n",
    "            qb_mask |= (fdf[\"player_role\"] == \"Passer\").to_numpy()\n",
    "        qb_pts = fdf.loc[qb_mask, [\"x\", \"y\"]].to_numpy()\n",
    "\n",
    "        # \n",
    "        regions, def_pts, off_pts, tgt_pts, def_ids, off_ids, tgt_ids = voronoi_polygons_for_frame_motion(\n",
    "            fdf, lookahead_sec=lookahead_sec\n",
    "        )\n",
    "\n",
    "        traces = []\n",
    "\n",
    "        # Voronoi ownership polygons\n",
    "        for reg in regions:\n",
    "            color = COLOR_DEF if reg[\"team\"] == \"Defense\" else COLOR_OFF\n",
    "            traces.append(go.Scatter(\n",
    "                x=reg[\"x\"] + [reg[\"x\"][0]],\n",
    "                y=reg[\"y\"] + [reg[\"y\"][0]],\n",
    "                mode=\"lines\",\n",
    "                line=dict(color=LINE_COLOR, width=1),\n",
    "                fill=\"toself\",\n",
    "                fillcolor=color,\n",
    "                hoverinfo=\"skip\",\n",
    "                showlegend=False\n",
    "            ))\n",
    "\n",
    "        # Build mapping\n",
    "        area_by_id = {}\n",
    "        for reg in regions:\n",
    "            area = polygon_area(reg[\"x\"], reg[\"y\"])\n",
    "            area_by_id[reg[\"nfl_id\"]] = area\n",
    "\n",
    "        # Areas aligned with defender/offense/targeted receiver dots\n",
    "        def_areas = [area_by_id.get(pid, np.nan) for pid in def_ids]\n",
    "        off_areas = [area_by_id.get(pid, np.nan) for pid in off_ids]\n",
    "        tgt_areas = [area_by_id.get(pid, np.nan) for pid in tgt_ids]\n",
    "\n",
    "        # Defensive convex hull on current positions\n",
    "        if len(def_pts) >= 3:\n",
    "            hull = ConvexHull(def_pts)\n",
    "            hull_pts = def_pts[hull.vertices]\n",
    "            traces.append(go.Scatter(\n",
    "                x=np.append(hull_pts[:, 0], hull_pts[0, 0]),\n",
    "                y=np.append(hull_pts[:, 1], hull_pts[0, 1]),\n",
    "                mode=\"lines\",\n",
    "                line=dict(color=\"rgba(0,0,200,0.8)\", width=2),\n",
    "                name=\"Defensive Hull\",\n",
    "                showlegend=bool(idx == 0)\n",
    "            ))\n",
    "        else:\n",
    "            traces.append(go.Scatter(x=[], y=[], mode=\"lines\", showlegend=False))\n",
    "\n",
    "        # Players (Defense)\n",
    "        if len(def_pts) > 0:\n",
    "            traces.append(go.Scatter(\n",
    "                x=def_pts[:, 0],\n",
    "                y=def_pts[:, 1],\n",
    "                mode=\"markers\",\n",
    "                marker=dict(color=\"blue\", size=6),\n",
    "                name=\"Defenders\",\n",
    "                showlegend=bool(idx == 0),\n",
    "                customdata=np.array(def_areas),\n",
    "                hovertemplate=(\n",
    "                    \"Space: %{customdata:.1f} yds²<br>\"\n",
    "                    \"X: %{x:.1f}<br>\"\n",
    "                    \"Y: %{y:.1f}<extra></extra>\"\n",
    "                ),\n",
    "            ))\n",
    "        else:\n",
    "            traces.append(go.Scatter(x=[], y=[], mode=\"markers\", showlegend=False))\n",
    "\n",
    "        # Players (no QB)\n",
    "        if len(off_pts) > 0:\n",
    "            traces.append(go.Scatter(\n",
    "                x=off_pts[:, 0],\n",
    "                y=off_pts[:, 1],\n",
    "                mode=\"markers\",\n",
    "                marker=dict(color=\"red\", size=6),\n",
    "                name=\"Offense\",\n",
    "                showlegend=bool(idx == 0),\n",
    "                customdata=np.array(off_areas),\n",
    "                hovertemplate=(\n",
    "                    \"Space: %{customdata:.1f} yds²<br>\"\n",
    "                    \"X: %{x:.1f}<br>\"\n",
    "                    \"Y: %{y:.1f}<extra></extra>\"\n",
    "                ),\n",
    "            ))\n",
    "        else:\n",
    "            traces.append(go.Scatter(x=[], y=[], mode=\"markers\", showlegend=False))\n",
    "\n",
    "        # Quarterback marker\n",
    "        if len(qb_pts) > 0:\n",
    "            traces.append(go.Scatter(\n",
    "                x=qb_pts[:, 0],\n",
    "                y=qb_pts[:, 1],\n",
    "                mode=\"markers\",\n",
    "                marker=dict(color=\"purple\", size=10, symbol=\"diamond\"),\n",
    "                name=\"Quarterback\",\n",
    "                showlegend=bool(idx == 0),\n",
    "                hovertemplate=(\n",
    "                    \"Quarterback<br>\"\n",
    "                    \"X: %{x:.1f}<br>\"\n",
    "                    \"Y: %{y:.1f}<extra></extra>\"\n",
    "                ),\n",
    "            ))\n",
    "        else:\n",
    "            traces.append(go.Scatter(x=[], y=[], mode=\"markers\", showlegend=False))\n",
    "\n",
    "        # Targeted Receiver\n",
    "        if len(tgt_pts) > 0:\n",
    "            traces.append(go.Scatter(\n",
    "                x=tgt_pts[:, 0],\n",
    "                y=tgt_pts[:, 1],\n",
    "                mode=\"markers\",\n",
    "                marker=dict(color=\"gold\", size=14, symbol=\"star\"),\n",
    "                name=\"Targeted Receiver\",\n",
    "                showlegend=bool(idx == 0),\n",
    "                customdata=np.array(tgt_areas),\n",
    "                hovertemplate=(\n",
    "                    \"Targeted Receiver<br>\"\n",
    "                    \"Space: %{customdata:.1f} yds²<br>\"\n",
    "                    \"X: %{x:.1f}<br>\"\n",
    "                    \"Y: %{y:.1f}<extra></extra>\"\n",
    "                ),\n",
    "            ))\n",
    "        else:\n",
    "            traces.append(go.Scatter(x=[], y=[], mode=\"markers\", showlegend=False))\n",
    "\n",
    "        # Ball Landing\n",
    "        if ball_x is not None and ball_y is not None:\n",
    "            traces.append(go.Scatter(\n",
    "                x=[ball_x], y=[ball_y],\n",
    "                mode=\"markers\",\n",
    "                marker=dict(color=\"brown\", size=10, symbol=\"circle\"),\n",
    "                name=\"Ball Landing\",\n",
    "                showlegend=bool(idx == 0)\n",
    "            ))\n",
    "        else:\n",
    "            traces.append(go.Scatter(x=[], y=[], mode=\"markers\", showlegend=False))\n",
    "\n",
    "        frames_out.append(go.Frame(name=str(fid), data=traces))\n",
    "\n",
    "    base_data = frames_out[0].data if frames_out else []\n",
    "\n",
    "    fig = go.Figure(\n",
    "        data=base_data,\n",
    "        frames=frames_out,\n",
    "        layout=go.Layout(\n",
    "            title=title,\n",
    "            xaxis=dict(range=[FIELD_X_MIN, FIELD_X_MAX],\n",
    "                       title=\"X (yards)\", fixedrange=True),\n",
    "            yaxis=dict(range=[FIELD_Y_MIN, FIELD_Y_MAX],\n",
    "                       title=\"Y (yards)\", fixedrange=True, scaleanchor=\"x\"),\n",
    "            width=950,\n",
    "            height=480,\n",
    "            showlegend=True,\n",
    "\n",
    "            sliders=[{\n",
    "                \"x\": 0.10,\n",
    "                \"y\": -0.10,\n",
    "                \"len\": 0.8,\n",
    "                \"currentvalue\": {\"prefix\": \"Frame: \"},\n",
    "                \"steps\": [\n",
    "                    {\n",
    "                        \"label\": str(fid),\n",
    "                        \"method\": \"animate\",\n",
    "                        \"args\": [\n",
    "                            [str(fid)],\n",
    "                            {\n",
    "                                \"mode\": \"immediate\",\n",
    "                                \"frame\": {\"duration\": 0, \"redraw\": False},\n",
    "                                \"transition\": {\"duration\": 0}\n",
    "                            }\n",
    "                        ]\n",
    "                    }\n",
    "                    for fid in frame_ids\n",
    "                ]\n",
    "            }]\n",
    "        )\n",
    "    )\n",
    "\n",
    "    return fig"
   ]
