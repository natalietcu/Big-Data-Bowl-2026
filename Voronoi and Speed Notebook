    "frames_list = []\n",
    "\n",
    "for frame in frames:\n",
    "    frame_def = defense_data[defense_data[\"frame_id\"] == frame][[\"x\", \"y\"]].values\n",
    "    frame_target = target_data[target_data[\"frame_id\"] == frame][[\"x\", \"y\"]].values\n",
    "\n",
    "    # Defensive convex hull\n",
    "    hull_x, hull_y = [], []\n",
    "    if len(frame_def) >= 3:\n",
    "        hull = ConvexHull(frame_def)\n",
    "        hull_points = frame_def[hull.vertices]\n",
    "        hull_x = np.append(hull_points[:, 0], hull_points[0, 0]) \n",
    "        hull_y = np.append(hull_points[:, 1], hull_points[0, 1])\n",
    "\n",
    "    frame_trace = [\n",
    "        go.Scatter(\n",
    "            x=defense_data[defense_data[\"frame_id\"] == frame][\"x\"],\n",
