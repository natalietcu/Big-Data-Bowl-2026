   "source": [
    "def compute_space_ownership_over_time(play_df, lookahead_sec=0.5):\n",
    "\n",
    "    records = []\n",
    "    frame_ids = sorted(play_df[\"frame_id\"].unique())\n",
    "\n",
    "    has_game = \"game_id\" in play_df.columns\n",
    "    has_play = \"play_id\" in play_df.columns\n",
    "\n",
    "    for fid in frame_ids:\n",
    "        fdf = play_df[play_df[\"frame_id\"] == fid]\n",
    "\n",
    "        regions, def_pts, off_pts, tgt_pts, def_ids, off_ids, _ = voronoi_polygons_for_frame_motion(\n",
    "            fdf, lookahead_sec=lookahead_sec\n",
    "        )\n",
    "\n",
    "        for reg in regions:\n",
    "            area = polygon_area(reg[\"x\"], reg[\"y\"])\n",
    "\n",
    "            rec = {\n",
    "                \"frame_id\": fid,\n",
    "                \"nfl_id\": reg[\"nfl_id\"], \n",
    "                \"team\": reg[\"team\"],\n",
    "                \"area\": area,              # yards^2 of space owned\n",
    "            }\n",
    "            if has_game:\n",
    "                rec[\"game_id\"] = fdf[\"game_id\"].iloc[0]\n",
    "            if has_play:\n",
    "                rec[\"play_id\"] = fdf[\"play_id\"].iloc[0]\n",
    "\n",
    "            records.append(rec)\n",
    "\n",
    "    return pd.DataFrame(records)"
   ]
  },
