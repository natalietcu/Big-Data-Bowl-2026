   "outputs": [],
   "source": [
    "play_df = add_voronoi_area_by_frame(play_df)\n",
    "\n",
    "dt = 0.1\n",
    "\n",
    "df = play_df.sort_values([\"nfl_id\", \"frame_id\"]).copy()\n",
    "\n",
    "df[\"dA_dt\"] = (\n",
    "    df.groupby(\"nfl_id\")[\"vor_area\"].diff() / dt\n",
    ")"
   ]
