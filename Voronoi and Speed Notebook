   "source": [
    "def polygon_area(xs, ys):\n",
    "\n",
    "    # Build shapely polygon from Voronoi vertices\n",
    "    poly = Polygon(zip(xs, ys))\n",
    "\n",
    "    inter = poly.intersection(FIELD_POLY)\n",
    "\n",
    "    if inter.is_empty:\n",
    "        return 0.0\n",
    "\n",
    "    return inter.area "
   ]
  },
