    "            marker=dict(color=\"blue\", size=6),\n",
    "            name=\"Defenders\",\n",
    "            hoverinfo=\"none\"\n",
    "        ),\n",
    "        go.Scatter(\n",
    "            x=frame_target[:, 0] if len(frame_target) > 0 else [],\n",
    "            y=frame_target[:, 1] if len(frame_target) > 0 else [],\n",
